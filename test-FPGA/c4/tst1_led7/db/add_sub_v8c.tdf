--synthesis_resources = lut 12 
SUBDESIGN add_sub_v8c
( 
	cout	:	output;
	dataa[10..0]	:	input;
	datab[10..0]	:	input;
	result[10..0]	:	output;
) 
VARIABLE
	result_int[11..0]	:	WIRE;
BEGIN 
	result_int[] = (0, dataa[]) - (0, datab[]);
	result[] = result_int[10..0];
	cout = !result_int[11];
END;
